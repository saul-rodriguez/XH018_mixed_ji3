// HDL file for Lib - COUNTER_JI3 ,Cell - counter, View - functional
-amscompilefile "file:${IC_INVOKE_DIR}/COUNTER_JI3/counter/functional/verilog.v lib:COUNTER_JI3 cell:counter view:functional"

// HDL file for Lib - COUNTER_JI3 ,Cell - counter_stimulus, View - verilogams
-amscompilefile "file:${IC_INVOKE_DIR}/COUNTER_JI3/counter_stimulus/verilogams/verilog.vams lib:COUNTER_JI3 cell:counter_stimulus view:verilogams"

// Create required libraries 
-makelib TOP 
-makelib INV_HV 
-makelib INV_LP 
-makelib IORING 
-makelib HV_CELLS 
-makelib IO_CELLS_JIC3V 
-endlib 
